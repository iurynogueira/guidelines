{
  "useWorkspaces": true,
  "version": "independent",
  "npmClient": "npm",
  "command": {
    "init": {
      "exact": true
    },
    "version": {
      "conventionalCommits": true,
      "exact": true,
      "ignoreChanges": [
        "*.md"
      ],
      "message": "chore(release): publish"
    },
    "publish": {
      "graphType": "all"
    }
  }
}
